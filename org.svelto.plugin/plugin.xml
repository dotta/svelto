<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="org.svelto.plugin.Startup">
      </startup>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.svelto.plugin.SveltoPreferences"
            id="org.svelto.plugin.preferencesID"
            name="Svelto">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.svelto.plugin.SveltoPreferencesInitializer">
      </initializer>
   </extension>
</plugin>
